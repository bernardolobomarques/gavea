{
  "name": "Gavea Telecom",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "format": "prettier --write .",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:coverage:report": "vitest --coverage --reporter=html",
    "test:coverage:check": "vitest --coverage --reporter=lcov && lcov -c -d coverage -o coverage/lcov.info && lcov -c -d coverage -o coverage/lcov.info && genhtml coverage/lcov.info --output-directory coverage/html && open coverage/html/index.html"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@chakra-ui/theme-tools": "^2.2.8",
    "@emotion/css": "^11.13.5",
    "@emotion/keyframes": "^0.9.1",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "framer-motion": "^11.0.3",
    "gh-pages": "^6.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.21.3",
    "sharp": "^0.34.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "vite": "^5.0.12"
  }
}
